<div class="main posts-show">
  <div class="container">
    <div class="posts-show-item">
      <p>
        <%= @post.content %>
      </p>
      <div class="post-time">
        <%= @post.created_at %>
      </div>
        <div class="post-menus">
        <%= link_to "編集", edit_post_path %>
        <%= link_to "削除する", post_path, method: :delete,data:{confirm:"本当によろしいですか？"} %>
      </div>
    </div>
  </div>
</div>
